{
  "name": "bot-zero",
  "version": "1.0.3",
  "private": true,
  "author": "Wehkamp (kbakker@wehkamp.nl)",
  "description": "An 'empty' Slack Bot that can be the jump start for your own bot. It is based on the Hubot project.",
  "dependencies": {
    "axios": "^0.21.1",
    "cross-env": "^7.0.2",
    "hubot": "^3.3.2",
    "hubot-command-mapper": "^2.3.12",
    "hubot-help": "^1.0.1",
    "hubot-slack": "^4.8.1",
    "npm-force-resolutions": "0.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "hubot-pretend": "^1.2.0",
    "mocha": "^8.2.0"
  },
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "start": "node ./index.js",
    "test": "npm run mocha ./tests/**/*.spec.js",
    "mocha": "mocha --recursive --timeout 10000 --exit",
    "audit-fix": "npm audit fix --force & npm install & npx npm-force-resolutions & npm install"
  },
  "main": "index.js",
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wehkamp/bot-zero.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wehkamp/bot-zero/issues"
  },
  "homepage": "https://github.com/wehkamp/bot-zero#readme",
  "resolutions": {
    "morgan": "1.9.1",
    "base64-url": "2.3.2",
    "mime": "1.4.1",
    "debug": "3.1.0",
    "fresh": "0.5.2",
    "negotiator": "0.6.1",
    "ws": "3.3.1"
  }
}
