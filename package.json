{
  "name": "bot-zero",
  "version": "2.0.0",
  "private": true,
  "author": "Wehkamp (kbakker@wehkamp.nl)",
  "description": "An 'empty' Slack Bot that can be the jump start for your own bot. It is based on the Hubot project.",
  "dependencies": {
    "@slack/web-api": "^6.5.1",
    "@types/hubot": "^3.3.1",
    "chalk-with-markers": "^1.0.24",
    "cross-env": "^7.0.3",
    "hubot": "^3.3.2",
    "hubot-command-mapper": "^3.0.5",
    "hubot-help": "^1.0.1",
    "hubot-slack": "^4.5.1",
    "node-fetch": "^2.6.6",
    "npm": "^8.1.4",
    "npm-force-resolutions": "0.0.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "del-cli": "^3.0.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "npm run clean --silent & ts-node-dev --tree-kill --respawn --watch '*.json,src/**/*.ts,*.env' --transpile-only src/index.ts",
    "audit-fix": "npm audit fix --force & npm install & npx npm-force-resolutions & npm install",
    "build": "tsc",
    "clean": "del-cli dist",
    "test": "echo \"Bot Zero II has no tests...\" & exit 0"
  },
  "main": "dist/scr/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wehkamp/bot-zero.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wehkamp/bot-zero/issues"
  },
  "homepage": "https://github.com/wehkamp/bot-zero#readme",
  "resolutions": {
    "json-schema": "0.4.0",
    "ansi-regex": "6.0.1",
    "strip-ansi": "7.0.1",
    "string-width": "5.0.1"
  }
}
